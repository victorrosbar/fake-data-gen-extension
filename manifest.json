{
    "manifest_version": 3,
    "name": "Gerador de Dados Fict√≠cios",
    "version": "1.0",
    "description": "Gera CPF, CNPJ, telefone e email para testes.",
    "permissions": ["clipboardWrite", "storage"],
    "host_permissions": ["<all_urls>"] ,
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js", "data-gen.js"],
        "css": ["styles.css"]
      }
    ]
  }